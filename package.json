{
  "private": true,
  "workspaces": {
    "packages": [
      "api",
      "web",
      "bot"
    ]
  },
  "scripts": {
    "start": "tsc -p ./bot/ & node --env-file=.env ./bot/dist/bot.js ",
    "deploy": "tsc -p ./bot/ & node --env-file=.env ./bot/dist/deploy-commands.js",
    "build": "tsc -p ./bot/"
  },
  "devDependencies": {
    "@redwoodjs/core": "7.0.6",
    "@redwoodjs/project-config": "7.0.6"
  },
  "eslintConfig": {
    "extends": "@redwoodjs/eslint-config",
    "root": true
  },
  "engines": {
    "node": "=20.x"
  },
  "prisma": {
    "seed": "yarn rw exec seed"
  },
  "packageManager": "yarn@4.1.0"
}
